{
  "name": "maxserv/yaml_configuration",
  "type": "typo3-cms-extension",
  "description": "Configure your TYPO3 site using YAML files. Export and import any table to and from a YAML file.",
  "license": [
    "GPL-2.0"
  ],
	"authors": [
		{
			"name": "Michiel Roos",
			"email": "michiel@michielroos.com",
			"role": "Developer",
			"homepage": "https://www.michielroos.com"
		},
		{
			"name": "Josef Glatz",
			"email": "jousch@gmail.com",
			"role": "Developer",
			"homepage": "https://jousch.com"
		}
	],
  "require": {
    "typo3/cms-core": "^9.5",
    "symfony/yaml": "2.6 - 4.99"
  },
  "replace": {
    "yaml_configuration": "self.version",
    "typo3-ter/yaml_configuration": "self.version"
  },
  "autoload": {
    "psr-4": {
      "MichielRoos\\YamlConfiguration\\": "Classes/"
    }
  }
}
